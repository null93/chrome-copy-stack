{
	"manifest_version": 2,
	"name": "Copy Stack",
	"version": "1.0.1",
	"description": "Add selection to copy stack and reduce stack to a result",
	"permissions": [
		"storage",
		"activeTab"
	],
	"browser_action": {
		"default_title": "Copy Stack",
		"default_icon": "icon-128.png"
	},
	"icons": {
		"16":  "icon-16.png",
		"32":  "icon-32.png",
		"48":  "icon-48.png",
		"128": "icon-128.png"
	},
	"background": {
		"scripts": [ "background.js" ],
		"persistent": false
	}
}
