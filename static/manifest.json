{
	"manifest_version": 3,
	"name": "Copy Stack",
	"version": "1.0.2",
	"description": "Add selection to copy stack and reduce stack to a result",
	"permissions": [
		"storage",
		"activeTab",
		"scripting"
	],
	"action": {
		"default_title": "Copy Stack",
		"default_icon": "icon-128.png"
	},
	"icons": {
		"16":  "icon-16.png",
		"32":  "icon-32.png",
		"48":  "icon-48.png",
		"128": "icon-128.png"
	},
	"background": {
		"service_worker": "service_worker.js",
		"type": "module"
	}
}
